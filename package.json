  "author": "Maximilian Antoni <mail@maxantoni.de> (http://maxantoni.de)",
  "license": "MIT",
  "devDependencies": {
    "jslint": "^0.8.0",
    "animate.css": "^3.2.5",
    "browserify": "^10.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mantoni/animatify.git"
  }
}
